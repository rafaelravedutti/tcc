fn iterate(img : image_struct, body: fn(int, int) -> ()) -> () {
  let mut i = 0;
  let mut j = 0;

  while(i < img.width) {
    j = 0;

    while(j < img.height) {
      body(i, j);
      j = j + 1;
    }

    i = i + 1;
  }
}

fn sqrt(a : f64) -> (f64) {
  cpu_intrinsics.sqrt(a)
}

fn atan2(y : f64, x : f64) -> (f64) {
  cpu_intrinsics.atan2(y, x)
}
