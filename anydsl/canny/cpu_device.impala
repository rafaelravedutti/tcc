fn iterate(img : Image, body: fn(int,int) -> ()) -> () {
  let mut i = 0;
  let mut j = 0;

  while(i < img.height) {
    while(j < img.width) {
      body(i, j);
      j = j + 1;
    }

    i = i + 1;
  }
}

fn sqrtf(a : f32) -> (f32) {
  cpu_intrinsics.sqrtf(a)
}

fn atan2f(y : f32, x : f32) -> (f32) {
  cpu_intrinsics.atan2f(y, x)
}
